{
  "name": "nghtmled",
  "version": "0.1.0",
  "dependencies": {
    "angular": "^1.4.0"
  },
  "main": "app/dist/*.js",
  "install":{
    "path":"app"  
  },
  "ignore": [
    "source",
    "spec",
    ".bowerrc",
    ".gitignore",
    ".jshintignore",
    ".jshintrc",
    "bower.json",
    "gruntfile.js",
    "package.json",
    "README.md",
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
